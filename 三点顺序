#include<stdio.h>
int main()
{
	double x1,x2,x3,y1,y2,y3;
	while(scanf("%lf%lf%lf%lf%lf%lf",&x1,&y1,&x2,&y2,&x3,&y3))
	{
		if(x1==0&&x2==0&&x3==0&&y1==0&&y2==0&&y3==0)
			break;
		double t;
		t=(x2-x1)*(y3-y1)-(y2-y1)*(x3-x1);
		if(t>0)
		{
			printf("逆时针\n");
		}
		else
		{
			printf("顺时针\n");
		}
	}
  return 0;
}
